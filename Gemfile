source "https://rubygems.org"

gemspec

# Dependencies required for development
group :development do
  gem "bundler"
  gem "rspec"
end

# Dependencies required for testing
group :test do
  gem "rails", "~> 6.1"
  gem "database_cleaner"
  gem "factory_bot_rails"
  gem "shoulda-matchers"
  gem "simplecov"
  gem "sqlite3"
end